package main

import ("fmt"
	"io/ioutil"
	"os"
	"strconv"
)

func find_number(xs []uint8, start int) int {
	end := start
	for i := start; xs[i] != 10; i++ {
		end++
	}
	number, _ := strconv.Atoi(string(xs[start:end]))
	return number
}

func main() {
	dat, err := ioutil.ReadFile(os.Args[1])
	if err != nil {
		fmt.Print(err)
	}

	sum := 0
	for i, v := range dat {
		mul := 1;
		if v == 45  || v == 43 {
			if v == 45 {
				mul *= -1
			}
			sum += (mul * find_number(dat, i + 1))
		}
	}
	fmt.Println(sum)
}
